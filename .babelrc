{
  "presets": [
    [
      "@babel/preset-env",
      {
        "targets": {
          "node": "8",
          "browsers": "cover 99.5%"
        }
      }
    ],
    "@babel/preset-react"
  ],
  "plugins": [
    "@babel/plugin-proposal-class-properties",
    ["@babel/plugin-transform-parameters", {
      "loose": false
    }]
  ]
}
